{
  "rewrites": [
    { "source": "/", "destination": "/client/index.html" },
    { "source": "/index.html", "destination": "/client/index.html" },
    { "source": "/start.html", "destination": "/client/start.html" },
    { "source": "/chat.html", "destination": "/client/chat.html" },

    { "source": "/style.css", "destination": "/client/style.css" },
    { "source": "/script.js", "destination": "/client/script.js" },
    { "source": "/assets/(.*)", "destination": "/client/assets/$1" }
  ]
}
